//Area of Rectangle
#include<stdio.h>
    int main()
    {
    float l,b,a;
    //l refers to the length of the rectangle
    //b refers to th ebreadth of the rectangle
    //a refers to the area 
    printf("Enter the length of the rectangle: ");
    scanf("%f",&l);
    printf("Enter the breadth of the rectangle: ");
    scanf("%f",&b);
    a=(l*b);
    printf("The area of the rectangle with dimensions %.2f and %.2f is %.2f.",l,b,a);
    return 0;
    }
