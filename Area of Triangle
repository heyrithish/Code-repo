//to find area of triangle when three sides are given
#include<stdio.h>
#include<math.h>

int main()
{
    float a,b,c,s,area;
    printf("\nEnter the length of sides: ");
    scanf("%f%f%f",&a,&b,&c);
    s=(a+b+c)/2;
    area=sqrt(s*(s-a)*(s-b)*(s-c));
    printf("The area of the triangle with the dimensions %.2f x %.2f x %.2f is %.2f",a,b,c,area);
    return 0;

}
