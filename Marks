//To find total, average and percentage out of 5 subjects
#include<stdio.h>
int main()
{
    float s1,s2,s3,s4,s5,m,t,a,p;
    //Highest marks in each subject is 30.
    printf("\nEnter maximum marks for each subject: ");
    scanf("%f",&m);
    printf("\nEnter the marks in subject 1: ");
    scanf("%f",&s1);
    printf("\nEnter the marks in subject 2: ");
    scanf("%f",&s2);
    printf("\nEnter the marks in subject 3: ");
    scanf("%f",&s3);
    printf("\nEnter the marks in subject 4: ");
    scanf("%f",&s4);
    printf("\nEnter the marks in subject 5: ");
    scanf("%f",&s5);
    t=(s1+s2+s3+s4+s5);
    a=t/5;
    p=(t/(5*m))*100;
    printf("Total marks obtained is %g out of %g.\nAverage marks is %g.\nTotal percentage is %g.",t,(5*m),a,p);
    return 0;

}
